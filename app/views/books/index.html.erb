<h1>Books</h1>
<div class="index">
  
  
</div>
<h2>New book</h2>
<div class="form-contents">
   <%= form_with model:@book, url:'/books', local:true do |f| %>
     <div class="form">
       <h4>Title</h4>
       <%= f.text_field :title %>
     </div>
     <div class="form">
       <h4>Body</h4>
       <%= f.text_area :body %>
     </div>
     <div class="submit"><%= f.submit 'Create Book' %></div>
   <% end %>
</div>



